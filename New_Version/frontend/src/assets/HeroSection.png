imeo-video-wrapper"></div></div><div data-w-tab="Grow Your Email List" data-video-id="669435995" class="home-video-slider-pane w-tab-pane" id="w-tabs-1-data-w-pane-5" role="tabpanel" aria-labelledby="w-tabs-1-data-w-tab-5"><div class="vimeo-video-wrapper"></div></div><div data-w-tab="Gamify Your Campaign" data-video-id="669435390" class="home-video-slider-pane w-tab-pane" id="w-tabs-1-data-w-pane-6" role="tabpanel" aria-labelledby="w-tabs-1-data-w-tab-6"><div class="vimeo-video-wrapper"></div></div><div data-w-tab="Make Announcements" data-video-id="669435941" class="home-video-slider-pane w-tab-pane" id="w-tabs-1-data-w-pane-7" role="tabpanel" aria-labelledby="w-tabs-1-data-w-tab-7"><div class="vimeo-video-wrapper"></div></div><div data-w-tab="Increase User Engagement" data-video-id="669436216" class="home-video-slider-pane w-tab-pane" id="w-tabs-1-data-w-pane-8" role="tabpanel" aria-labelledby="w-tabs-1-data-w-tab-8"><div class="vimeo-video-wrapper"></div></div><div data-w-tab="Collect Form Submissions" data-video-id="669435701" class="home-video-slider-pane w-tab-pane" id="w-tabs-1-data-w-pane-9" role="tabpanel" aria-labelledby="w-tabs-1-data-w-tab-9"><div class="vimeo-video-wrapper"></div></div><div data-w-tab="Facilitate Social Sharing" data-video-id="669436129" class="home-video-slider-pane w-tab-pane" id="w-tabs-1-data-w-pane-10" role="tabpanel" aria-labelledby="w-tabs-1-data-w-tab-10"><div class="vimeo-video-wrapper"></div></div></div></div></div></section><div class="w-embed w-script"><script type="text/javascript">
const homeVideoSliderSection = document.querySelector('#home-video-slider-section');

const observer = new IntersectionObserver((entries, observer) => {
  entries.forEach(entry => {
    if (entry.isIntersecting) {
      const playerJS = document.createElement('script');
      playerJS.src = 'https://player.vimeo.com/api/player.js';
      playerJS.onload = startVimeoVideo; // Call startVimeoVideo after the script loads
      document.head.appendChild(playerJS);
      window.vimeoPlayerScriptLoaded = true; // Mark as loaded
      observer.unobserve(entry.target);
    }
  });
}, { threshold: 0.1 });

observer.observe(homeVideoSliderSection);

let player; // Global player variable

const createVimeoPlayer = (videoId, wrapper) => {
  const videoContainer = document.createElement('div');
  videoContainer.id = videoId;
  videoContainer.style.aspectRatio = '16/9';
  wrapper.appendChild(videoContainer);

  const options = {
    id: videoId,
    responsive: true,
    loop: true,
    muted: true,
    controls: false,
    allow: "autoplay; allowfullscreen",
    title: false,
    inline: false,
    portrait: false,
  };

  return new Vimeo.Player(videoId, options);
};

const startVimeoVideo = () => {
  const homeVideoSliderActiveContent = document.querySelector('.home-video-slider-content .w--tab-active');
  if (!homeVideoSliderActiveContent) return;

  const wrapper = homeVideoSliderActiveContent.querySelector('.vimeo-video-wrapper');
  const videoId = homeVideoSliderActiveContent.getAttribute('data-video-id');
  const currentVideoContainer = document.getElementById(videoId);

  if (!currentVideoContainer) {
    player = createVimeoPlayer(videoId, wrapper);
  } else {
    player = new Vimeo.Player(currentVideoContainer);
    player.setCurrentTime(0);
  }

  player.ready().then(() => player.play());
};

// Setup event listeners for tab changes
document.querySelectorAll('.home-video-slider-tab-link').forEach((tab) => {
  tab.addEventListener('click', () => {
    setTimeout(startVimeoVideo, 150);
